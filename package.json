{
  "name": "gtd",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "test": "npm run format:check && npm run lint",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "verify": "NODE_NO_WARNINGS=1 node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' scripts/verify.mjs",
    "test-setup": "NODE_NO_WARNINGS=1 node scripts/test.mjs"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@aws-sdk/client-s3": "^3.685.0",
    "@aws-sdk/client-secrets-manager": "^3.686.0",
    "@aws-sdk/s3-request-presigner": "^3.685.0",
    "@radix-ui/react-icons": "^1.3.1",
    "convex": "^1.17.0",
    "framer-motion": "^11.11.11",
    "langchain": "^0.3.5",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "shadcn-ui": "^0.9.3",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@pinecone-database/pinecone": "^4.0.0",
    "@types/node": "^20.17.6",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
